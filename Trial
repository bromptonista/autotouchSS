function qgpage_yes()
  local region = {5, 126, 98, 100};
  local result_qgpage = findImage("images/qg_rang.png", 1, 0.7, nil, region);
  local found = false
  for i,v in pairs(result_qgpage) do
    if v[1] ~= nil and v[2] ~= nil then
      found = true
    end
  end
  return found
end

rangFound = qgpage_yes()
log(rangFound)

